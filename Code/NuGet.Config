    <add key="nuget.org" value="https://api.nuget.org/v3/index.json" protocolVersion="3" />
    <!--Note: needs credentials to use from docker build.  See below. Probably also some way to pass in via cmdline, instead.-->
    <add key="AvalancheNuGet" value="https://ismclouddev.pkgs.visualstudio.com/avalanche/_packaging/AvalancheNuGet/nuget/v3/index.json" />
  </packageSources>
  <packageSourceCredentials>
